services:
  basket.api:
    build:
      context: D:\Try\APIGateway
      dockerfile: src/Basket/Basket.API/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Basket.API
      target: base
    container_name: Basket.API
    depends_on:
    - basketdb
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ConnectionStrings:Redis: basketdb:6379
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      EventBus:HostName: rabbitmq
      NUGET_FALLBACK_PACKAGES: ''
    image: basketapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  ""'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 8001
      target: 80
    tty: true
    volumes:
    - D:\Try\APIGateway\src\Basket\Basket.API:/app:rw
    - C:\Users\Admin\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\Admin\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - /.aspnet/https:/root/.aspnet/https/:rw
    - C:\Users\Admin\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\Admin\.nuget\packages:/root/.nuget/packages:ro
    - /.microsoft/usersecrets:/root/microsoft/usersecrets:rw
    - D:\Try\APIGateway:/src:rw
  basketdb:
    container_name: basketdb
    image: redis
    ports:
    - published: 6379
      target: 6379
    restart: always
  catalog.api:
    build:
      context: D:\Try\APIGateway
      dockerfile: src/Catalog/Catalog.API/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: Catalog.API
      target: base
    container_name: Catalog.API
    depends_on:
    - catalogdb
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      CatalogDatabaseSettings:ConnectionString: mongodb://catalogdb:27017
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: catalogapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  ""'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 8000
      target: 80
    tty: true
    volumes:
    - D:\Try\APIGateway\src\Catalog\Catalog.API:/app:rw
    - C:\Users\Admin\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\Admin\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - /.aspnet/https:/root/.aspnet/https/:rw
    - C:\Users\Admin\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\Admin\.nuget\packages:/root/.nuget/packages:ro
    - /.microsoft/usersecrets:/root/microsoft/usersecrets:rw
    - D:\Try\APIGateway:/src:rw
  catalogdb:
    container_name: catalogdb
    image: mongo
    ports:
    - published: 27017
      target: 27017
    restart: always
    volumes:
    - /site:/data/db:rw
  orderdb:
    container_name: orderdb
    environment:
      ACCEPT_EULA: "Y"
      SA_PASSWORD: Roh@bhi03
    image: mcr.microsoft.com/mssql/server
    ports:
    - published: 1433
      target: 1433
  ordering.api:
    build:
      context: D:\Try\APIGateway
      dockerfile: src/Ordering/Ordering.API/Dockerfile
    container_name: orderingapi
    depends_on:
    - orderdb
    - rabbitmq
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ConnectionStrings:OrderConnection: Server=orderdb;Database=OrderDb;User Id=sa;Password=Roh@bhi03
      EventBus:HostName: rabbitmq
    image: orderingapi
    ports:
    - published: 8002
      target: 80
    restart: on-failure
    volumes:
    - /.aspnet/https:/root/.aspnet/https/:rw
    - /.microsoft/usersecrets:/root/microsoft/usersecrets:rw
  rabbitmq:
    container_name: rabbitmq
    image: rabbitmq:3-management-alpine
    ports:
    - published: 5672
      target: 5672
    - published: 15672
      target: 15672
    restart: always
version: '3.4'